<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ValidateProtocol</fullName>
    <active>true</active>
    <errorConditionFormula>
    !BEGINS(PhotoUrl__c, 'https')
    </errorConditionFormula>
    <errorMessage>Pholo url should use https protocol</errorMessage>
</ValidationRule>